    "test": "mocha --require @babel/register spec/*.js spec/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trevSmart/rally-node.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "lodash": "^4.17.21",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/register": "^7.28.3",
    "@babel/eslint-parser": "^7.28.4",
    "@eslint/js": "^9.36.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "mocha": "^11.7.2",
    "should": "^13.2.3",
    "sinon": "^21.0.0"
  },
