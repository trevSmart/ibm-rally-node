{
  "name": "ibm-rally-node",
  "version": "0.0.1",
  "description": "Customized Rally REST Toolkit for Node.js",
  "keywords": [
    "rally",
    "api",
    "rest",
    "client",
    "agile",
    "alm"
  ],
  "contributors": [
    {
      "name": "Kyle Morse",
      "email": "kyle.morse@ca.com"
    },
